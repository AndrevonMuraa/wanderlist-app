----+    if not comment:
----+        raise HTTPException(status_code=404, detail="Comment not found")
----+    
----+    # Check if already liked
----+    existing_like = await db.comment_likes.find_one({
----+        "comment_id": comment_id,
----+        "user_id": current_user.user_id
----+    })
----+    
